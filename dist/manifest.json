{
  "manifest_version": 2,
  "name": "tbd",
  "description": "A hidden layer of the internet.",
  "version": "0.0.0",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["storage", "http://localhost:5000/*", "http://*/*", "http://*/", "http://*/*/*"]
}